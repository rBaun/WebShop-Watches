        xmlns:local="clr-namespace:DesktopClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="800">
    <Grid>
        <TextBox x:Name="emploEmail"  HorizontalAlignment="Left" Height="36" Margin="247,100,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="307" RenderTransformOrigin="-0.405,-2.261" FontSize="20" />
