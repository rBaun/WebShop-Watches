        <Button Content="Login" HorizontalAlignment="Left" Margin="440,245,0,0" VerticalAlignment="Top" Width="114" Height="29" Click="Button_Click_1"/>
        <Label Content="Indtast din email" HorizontalAlignment="Left" Margin="320,56,0,0" VerticalAlignment="Top" Height="32" Width="149" FontSize="18"/>
        <Label Content="Indtast password" HorizontalAlignment="Left" Margin="320,141,0,0" VerticalAlignment="Top" Height="32" Width="149" FontSize="18"/>
        <Label x:Name="wrongLogin" Content="" HorizontalAlignment="Left" Margin="247,10,0,0" VerticalAlignment="Top" Height="41" Width="418" FontSize="20" Foreground="#FFFA0606"/>


    </Grid>

</Window>
